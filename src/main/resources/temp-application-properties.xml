<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
	<comment>application-properties</comment>

	<entry key="environment">DEVELOPMENT</entry>

	<!-- Site -->
	<entry key="resources.cache.seconds">0</entry>
	
	<!-- Database -->
	<entry key="database.driverClassName">com.mysql.jdbc.Driver</entry>
	<entry key="database.url">jdbc:mysql://127.0.0.1/slipp?uniCode=true&amp;characterEncoding=utf8</entry>
	<entry key="database.username">username</entry>
	<entry key="database.password">password</entry>
	
	<!-- WikiDatabase -->
	<entry key="wiki.database.driverClassName">com.mysql.jdbc.Driver</entry>
	<entry key="wiki.database.url">jdbc:mysql://127.0.0.1/wiki?uniCode=true&amp;characterEncoding=utf8</entry>
	<entry key="wiki.database.username">username</entry>
	<entry key="wiki.database.password">password</entry>	
	
	<!-- URL -->
	<entry key="application.url">http://localhost:8080</entry>
	
	<!-- security -->
	<entry key="socialsignin.signUpView">/signUpForm</entry>
	
	<entry key="twitter.consumerKey">consumerKey</entry>	
	<entry key="twitter.consumerSecret">consumerSecret</entry>

	<entry key="facebook.email">email</entry>
	<entry key="facebook.password">password</entry>
	<entry key="facebook.clientId">client</entry>
	<entry key="facebook.clientSecret">clientSecret</entry>
	
	<entry key="google.clientId">clientId</entry>
	<entry key="google.clientSecret">clientSecret</entry>	
	
	<entry key="naver.consumerKey">consumerKey</entry>	
	<entry key="naver.consumerSecret">consumerSecret</entry>

	<entry key="slipp.remember.token.key">rememberkey</entry>
	
	<!-- Attachment -->
	<entry key="attachment.root.dir">/home/user</entry>
</properties>